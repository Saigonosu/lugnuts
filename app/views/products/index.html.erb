<p id="notice"><%= notice %></p>

<h1>Products</h1>

  <% @products.each do |product| %>
    <div class='product'>
      <%= link_to image_tag(product.productlines.image || 'missing.jpg' ), product_path(product) %>
      <ul>
        <li><%= product.productName %></li>
        <li><%= product.MSRP %></li>
      </ul>
    </div>
  <% end %>

<br>

<%= link_to 'New Product', new_product_path %>
